<!DOCTYPE html>
<html>
<head>
  <title>Video Library</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="wrapper">
    <div id="container">
      <%= render :partial => 'layouts/header' %>
      <%= render :partial => 'layouts/sidebar' %>
      <section id="content">
        <%= flash_messages %>
        <%= yield %>
      </section>
    </div>
    <div id="content_footer"></div>
  </div>
  <%= render :partial => 'layouts/footer' %>

</body>
</html>
